
public class App {
	public static void main(String[] args) {
		
		Machine mach1 = new Machine();
		mach1.start();
		mach1.showinfo();
		
		Person person1 = new Person("Bob");
		person1.greet();
		
		Info info1 = new Machine();
		info1.showinfo();
		
		Info info2 = person1;
		info2.showinfo();
	}
}
